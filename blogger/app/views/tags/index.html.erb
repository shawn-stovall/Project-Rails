<h1>Tags</h1>

<ul id="tags" >
    <% @tags.each do |tag| %>
	<li>
	    <%= link_to tag.name, tag_path(tag),
	    class: 'tag_name', id:"tag_#{tag.id}" %>
	    <%= link_to "Delete", tag_path(tag),
	    method: :delete, data: {confirm: "Really delete the tag?"} %>
	</li>
    <% end %>
    <%= link_to "<< Back to Articles", articles_path %>
</ul>
